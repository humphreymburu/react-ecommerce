{
  "name": "phonez",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.2.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.4.1",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "jquery": "^3.4.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --importer=node_modules/node-sass-tilde-importer src/styles/sass -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/styles/css --watch",
    "start-js": "react-scripts start -p watch-css start-js",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
